<%- include ../partials/header.ejs %>
  <div class="login">
    <p class="user-exists-message">
    </p>
    <p class="auth-warning">
    </p>
    <br/>
    <br/>
    <p>Don't have an account? <a href="signup">Sign Up</a></p>
    <form action="/login" method="post">
